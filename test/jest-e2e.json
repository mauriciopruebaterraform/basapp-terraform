{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@src/(.*)": "<rootDir>/../src/$1",
    "@prisma/(.*)$": "<rootDir>/../node_modules/.prisma/$1"
  },
  "globalSetup": "<rootDir>/globalSetup.ts",
  "globalTeardown": "<rootDir>/globalTeardown.ts",
  "testTimeout": 10000
}